(module gben.tests.ratio-number)

(def num 3/5)
(def num2 4/7)

(println num)
(println (+ num num2))
(println (norm (+ num num2)))
(println (- num num2))
(println (norm (- num num2)))
(println (* num num2))
(println (norm (* num num2)))
(println (/ num num2))
(println (norm (/ num num2)))

(println (norm 2/10))

(printf "Over: %d, Under: %d\n" [(:over num) (:under num)])
