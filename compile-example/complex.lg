(module gben.tests.ComplexNumberModule)

(def num #C(3 5))
(def num2 #C(4.2 7.6))
(def num3 #C(3/5 6/7))

(println num)
(println num2)
(println (+ num num2))
(println (- num num2))
(println (* num num2))
(println (/ num num2))

(def mult-result (* num num3))
(println mult-result)
(println (norm (:real mult-result)))
(println (norm (:imagine mult-result)))

(printf "Real: %d, Imagine: %d\n" [(:real num) (:imagine num)])
